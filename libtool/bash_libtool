LIBTOOL_HOME="$HOME/local/libtool"

export PATH=$LIBTOOL_HOME/bin:$PATH
export LD_LIBRARY_PATH=$LIBTOOL_HOME/lib:$LD_LIBRARY_PATH
export LIBRARY_PATH=$LIBTOOL_HOME/lib:$LIBRARY_PATH
export C_INCLUDE_PATH=$LIBTOOL_HOME/include:$C_INCLUDE_PATH
export CPLUS_INCLUDE_PATH=$LIBTOOL_HOME/include:$CPLUS_INCLUDE_PATH
